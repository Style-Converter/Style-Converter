{
    "components": {
      "primary-button": {
        "styles": {
          "appearance": "none",
          "caret-color": "transparent",
          "color-scheme": "light dark",
          "direction": "ltr",
          "forced-color-adjust": "none",
          "print-color-adjust": "exact",
          "speak-as": "literal-punctuation",
          "text-size-adjust": "100%",
          "unicode-bidi": "isolate",
          "user-select": "none"
        },
        "selectors": [
          {
            "when": ":hover",
            "styles": {
              "caret-color": "auto",
              "user-select": "text",
              "forced-color-adjust": "auto"
            }
          },
          {
            "when": ":active",
            "styles": {
              "appearance": "button",
              "speak-as": "digits",
              "unicode-bidi": "bidi-override"
            }
          }
        ],
        "media": [
          {
            "query": "(min-width: 768px)",
            "styles": {
              "color-scheme": "only dark",
              "text-size-adjust": "none",
              "user-select": "all"
            }
          }
        ]
      },
  
      "secondary-button": {
        "styles": {
          "appearance": "textfield",
          "caret-color": "red",
          "color-scheme": "dark light",
          "direction": "rtl",
          "forced-color-adjust": "preserve-parent-color",
          "print-color-adjust": "economy",
          "speak-as": "spell-out",
          "text-size-adjust": "auto",
          "unicode-bidi": "normal",
          "user-select": "auto"
        },
        "selectors": [
          {
            "when": ":hover",
            "styles": {
              "appearance": "none",
              "caret-color": "blue",
              "forced-color-adjust": "auto",
              "unicode-bidi": "plaintext"
            }
          }
        ]
      },
  
      "danger-button": {
        "styles": {
          "appearance": "menulist-button",
          "caret-color": "green",
          "color-scheme": "only light",
          "direction": "ltr",
          "forced-color-adjust": "none",
          "print-color-adjust": "exact",
          "speak-as": "literal-punctuation",
          "text-size-adjust": "150%",
          "unicode-bidi": "isolate-override",
          "user-select": "contain"
        },
        "selectors": [
          {
            "when": ":hover",
            "styles": {
              "caret-color": "yellow",
              "speak-as": "digits",
              "user-select": "none"
            }
          }
        ]
      },
  
      "input-field": {
        "styles": {
          "appearance": "none",
          "caret-color": "black",
          "color-scheme": "light dark",
          "direction": "auto",
          "forced-color-adjust": "auto",
          "print-color-adjust": "economy",
          "speak-as": "normal",
          "text-size-adjust": "100%",
          "unicode-bidi": "embed",
          "user-select": "text"
        },
        "selectors": [
          {
            "when": ":focus",
            "styles": {
              "caret-color": "orange",
              "user-select": "all",
              "unicode-bidi": "bidi-override"
            }
          }
        ]
      },
  
      "card": {
        "styles": {
          "appearance": "none",
          "caret-color": "transparent",
          "color-scheme": "light",
          "direction": "ltr",
          "forced-color-adjust": "preserve-parent-color",
          "print-color-adjust": "exact",
          "speak-as": "spell-out",
          "text-size-adjust": "auto",
          "unicode-bidi": "isolate",
          "user-select": "none"
        },
        "selectors": [
          {
            "when": ":hover",
            "styles": {
              "color-scheme": "dark",
              "user-select": "contain"
            }
          }
        ]
      },
  
      "modal": {
        "styles": {
          "appearance": "none",
          "caret-color": "purple",
          "color-scheme": "only dark",
          "direction": "rtl",
          "forced-color-adjust": "none",
          "print-color-adjust": "economy",
          "speak-as": "literal-punctuation",
          "text-size-adjust": "none",
          "unicode-bidi": "plaintext",
          "user-select": "auto"
        }
      },
  
      "navbar": {
        "styles": {
          "appearance": "none",
          "caret-color": "transparent",
          "color-scheme": "light",
          "direction": "ltr",
          "forced-color-adjust": "auto",
          "print-color-adjust": "economy",
          "speak-as": "normal",
          "text-size-adjust": "100%",
          "unicode-bidi": "isolate",
          "user-select": "none"
        }
      },
  
      "footer": {
        "styles": {
          "appearance": "none",
          "caret-color": "auto",
          "color-scheme": "dark",
          "direction": "rtl",
          "forced-color-adjust": "none",
          "print-color-adjust": "exact",
          "speak-as": "spell-out",
          "text-size-adjust": "auto",
          "unicode-bidi": "bidi-override",
          "user-select": "all"
        }
      },
  
      "badge": {
        "styles": {
          "appearance": "none",
          "caret-color": "transparent",
          "color-scheme": "only light",
          "direction": "ltr",
          "forced-color-adjust": "auto",
          "print-color-adjust": "economy",
          "speak-as": "digits",
          "text-size-adjust": "80%",
          "unicode-bidi": "embed",
          "user-select": "none"
        }
      },
  
      "alert": {
        "styles": {
          "appearance": "none",
          "caret-color": "black",
          "color-scheme": "light dark",
          "direction": "ltr",
          "forced-color-adjust": "none",
          "print-color-adjust": "exact",
          "speak-as": "literal-punctuation",
          "text-size-adjust": "auto",
          "unicode-bidi": "normal",
          "user-select": "contain"
        },
        "variants": {
          "success": {
            "caret-color": "green",
            "forced-color-adjust": "auto",
            "speak-as": "spell-out"
          },
          "error": {
            "caret-color": "red",
            "user-select": "none",
            "speak-as": "digits"
          },
          "warning": {
            "caret-color": "orange",
            "text-size-adjust": "150%",
            "unicode-bidi": "bidi-override"
          },
          "info": {
            "caret-color": "blue",
            "user-select": "auto",
            "speak-as": "normal"
          }
        }
      },
  
      "tooltip": {
        "styles": {
          "appearance": "tooltip",
          "caret-color": "white",
          "color-scheme": "dark",
          "direction": "ltr",
          "forced-color-adjust": "auto",
          "print-color-adjust": "economy",
          "speak-as": "normal",
          "text-size-adjust": "none",
          "unicode-bidi": "isolate-override",
          "user-select": "none"
        }
      }
    }
  }
  